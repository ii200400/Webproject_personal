<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->

<head>
	<%- include('head') -%>
</head>

<body>
	<div id="fh5co-wrapper">
		<div id="fh5co-page">
			<div id="fh5co-header">
				<%- include('header') -%>
			</div>
			<!-- end:fh5co-header -->
			<div class="container">
				<div id="fh5co-work-section">
					<div class="container" style="margin-top:100px;">
						<h1 style="font-size:40px;font-weight:500;">공지사항</h1>
						<table class="table table-board table-striped table-hover" style="margin-top:50px">
							<colgroup>
								<col width="7%" <!--글 번호-->
								<col width="20%" <!--제목-->
								<col width="10%" <!--작성자-->
								<col width="10%" <!--작성일-->
							</colgroup>
							<thead>
								<tr>
									<th style="text-align:center;">글 번호</th>
									<th style="text-align:center;">제목</th>
									<th style="text-align:center;">작성자</th>
									<th style="text-align:center;">작성일</th>
								</tr>
							</thead>
							<tbody>
								<%
									for(var i = rows.length - 1; i>=0; i--){
									var d = new Date(rows[i].created)
									var date = d.getFullYear() + "." + d.getMonth() + "." + d.getDate() %>
									<tr class="user_notice">
										<td style="text-align:center;"><%= i+1 %></td>
										<td><%= rows[i].title %></td>
										<td style="text-align:center;"><%= rows[i].author %></td>
										<td style="text-align:center;"><%= date %></td>
									</tr>
								<% } %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
			<%- include("footer") -%>
		</div>
		<!-- END fh5co-page -->
	</div>
	<!-- END fh5co-wrapper -->

	<!-- jQuery -->


	<script src="js/jquery.min.js"></script>
	<!-- jQuery Easing -->
	<script src="js/jquery.easing.1.3.js"></script>
	<!-- Bootstrap -->
	<script src="js/bootstrap.min.js"></script>
	<!-- Waypoints -->
	<script src="js/jquery.waypoints.min.js"></script>
	<!-- Superfish -->
	<script src="js/hoverIntent.js"></script>
	<script src="js/superfish.js"></script>
	<!-- Flexslider -->
	<script src="js/jquery.flexslider-min.js"></script>

	<!-- Main JS (Do not remove) -->
	<script src="js/main.js"></script>

	<script src="/js/admin/table_controller.js"></script>

</body>

</html>
